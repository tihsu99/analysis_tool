  "met_pt":{
    "Def": "MC_Data_Dep",
    "Label": ["Normal"],
    "Category": {"MC": "MET_T1Smear_pt_v",
                 "Data": "MET_T1_pt"}
  },

  "met_phi":{
    "Def": "MC_Data_Dep",
    "Label": ["Normal"],
    "Category": {"MC": "MET_T1Smear_phi_v",
                 "Data": "MET_T1_pt"}
  },

  "MET_reco_pz":{
    "Def": "MET_pz_reconstruction(Lepton_pt, Lepton_eta, Lepton_phi, met_pt, met_phi)",
    "Label": ["Reco","Normal"]
  },

  "MET_reco_E":{
    "Def": "sqrt(met_pt*met_pt + MET_reco_pz*MET_reco_pz)",
    "Label": ["Reco", "Normal"]
  },

  "Lepton_E":{
    "Def": "sqrt(Lepton_pt*Lepton_pt*(cosh(Lepton_eta)*cosh(Lepton_eta)) + Lepton_mass*Lepton_mass)",
    "Label": ["Reco", "Normal"]
  },

  "W_reco_E":{
    "Def": "MET_reco_E + Lepton_E",
    "Label": ["Reco", "Normal"]
  },

  "W_reco_px":{
    "Def": "Lepton_pt*cos(Lepton_phi) + met_pt*cos(met_phi)",
    "Label": ["Reco", "Normal"]
  },

  "W_reco_py":{
    "Def": "Lepton_pt*sin(Lepton_phi) + met_pt*sin(met_phi)",
    "Label": ["Reco", "Normal"]
  },

  "W_reco_pz":{
    "Def": "Lepton_pt*sinh(Lepton_eta) + MET_reco_pz",
    "Label": ["Reco", "Normal"]
  },

  "W_reco_mass":{
    "Def": "sqrt(W_reco_E*W_reco_E - W_reco_px*W_reco_px - W_reco_py*W_reco_py - W_reco_pz*W_reco_pz)",
    "Label": ["Reco", "Normal"]
  },

  "top_reco_mass_medium":{
    "Def": "top_reconstruction(W_reco_E, W_reco_px, W_reco_py, W_reco_pz, tightJets_b_DeepJetmedium_id, Jet_pt_nom, Jet_eta, Jet_phi, Jet_mass_nom)",
    "Label": ["Reco", "Normal"]
  },

  "top_reco_mass_tight":{
    "Def": "top_reconstruction(W_reco_E, W_reco_px, W_reco_py, W_reco_pz, tightJets_b_DeepJettight_id, Jet_pt_nom, Jet_eta, Jet_phi, Jet_mass_nom)",
